###################################################

@NODES _ROOT

@PRE
<2,2> varz("NL");

@RULES
_LINE <-
    _xSTART			### (1)
    _xWILD [plus]	### (2)
    @@

@PRE
<1,1> var("NL");
<2,2> varz("NL");

@RULES
_LINE <-
    _xWILD [one]	### (1)
    _xWILD [star]	### (2)
    @@
